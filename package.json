{
    "name": "douglasoward-portfolio",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "anchorate": "^1.2.3",
        "express": "^4.17.3",
        "gulp": "^4.0.2",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.3",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.87.3"
    },
    "eslintConfig": {
        "plugins": [
            "prettier"
        ],
        "extends": [
            "airbnb-base",
            "react-app",
            "plugin:prettier/recommended",
            "eslint-config-prettier"
        ],
        "env": {
            "browser": true
        },
        "settings": {
            "import/extensions": [
                ".js",
                ".jsx"
            ]
        },
        "parserOptions": {
            "ecmaVersion": 6,
            "ecmaFeatures": {
                "jsx": true
            }
        },
        "rules": {
            "prettier/prettier": "error",
            "import/no-unresolved": "off",
            "import/extensions": "off"
        }
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
